{
  "manifest_version": 3,
  "name": "flexunits",
  "description": "The extension helps to convert pixels into viewport units from figma layouts.",
  "version": "1.2.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.figma.com/*"
      ],
      "js": ["flexunits.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
